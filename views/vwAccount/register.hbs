<script>

  function validate() {
    return true;
  }
</script>
<header>
  <!-- Navbar -->
  {{>home/header}}
  <!-- Navbar -->
  <!--Intro Section-->
  <section class="view intro-2">
    <div class="mask rgba-gradient">
      <div class="container h-100 d-flex justify-content-center align-items-center">
        <!--Grid row-->
        <div class="row  pt-5 mt-3">
          <!--Grid column-->
          <div class="col-md-12">
            <!-- Material form register -->
            <div class="card">
              <h5 class="card-header info-color white-text text-center py-4">
                <strong>Sign up</strong>
              </h5>
              <!--Card content-->
              <div class="card-body px-lg-5 pt-0">
                <!-- Form -->
                <form class="text-center" style="color: #757575;" method="POST" onsubmit="return validate();" novalidate
                  id="frmRegister" name="frmRegister">
                  <div class="form-row">
                    <div class="col">
                      <!-- Username -->
                      <div class="md-form">
                        <input type="text" class="form-control" name="f_Username">
                        <label>Username</label>
                      </div>
                    </div>
                    <div class="col">
                      <!-- password -->
                      <div class="md-form">
                        <input type="password" class="form-control" name="raw_password">
                        <label>Password</label>
                      </div>
                    </div>
                  </div>
                  <!-- E-mail -->
                  <div class="form-row">
                    <div class="col">
                      <!-- password -->
                      <div class="md-form">
                        <input type="text" class="form-control" name="f_Name">
                        <label>Fullname</label>
                      </div>
                    </div>
                    <div class="col">
                      <!-- Username -->
                      <div class="md-form">
                        <input type="text" class="form-control" name="f_Email">
                        <label>Email</label>
                      </div>
                    </div>
                  </div>
                  <!-- Password -->
                  <div class="md-form">
                    <input type="text" class="form-control" name="f_address">
                    <label>Address</label>
                  </div>
                  <div class="md-form">
                    <input type="date" class="form-control" name="dob">
                    <label></label>
                  </div>
                  <!-- Phone number -->
                  <div class="md-form">
                    <input type="number" class="form-control" name="f_phone">
                    <label>Phone number</label>
                    <small id="materialRegisterFormPhoneHelpBlock" class="form-text text-muted mb-4">
                      Optional - for two step authentication
                    </small>
                  </div>
                  <!-- Newsletter -->
                  <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="materialRegisterFormNewsletter">
                    <label class="form-check-label" for="materialRegisterFormNewsletter">Subscribe
                      to our newsletter</label>
                  </div>
                  <div class="form-group">
                    <div class="g-recaptcha mx-auto col-md-8 my-4"
                      data-sitekey="6LffYTQUAAAAALMWZgHBUBIFoM4dELcVVoqiCItE" id="capcha" name="capcha"></div>
                  </div>
                  <!-- Sign up button -->
                  <button class="btn btn-outline-info btn-rounded btn-block my-4 waves-effect z-depth-0"
                    type="submit">Sign up</button>
                  <!-- Social register -->
                  <p>or sign up with:</p>
                  <a href="/account/auth/facebook" type="button" class="btn-floating btn-fb btn-sm">
                    <i class="fab fa-facebook-f"></i>
                  </a>
                  <a type="button" href="/account/auth/google" class="btn-floating btn-tw btn-sm">
                    <i class="fab fab fa-google"></i>
                  </a>
                  <a type="button" class="btn-floating btn-li btn-sm">
                    <i class="fab fa-linkedin-in"></i>
                  </a>
                  <a type="button" class="btn-floating btn-git btn-sm">
                    <i class="fab fa-github"></i>
                  </a>
                  <hr>
                  <!-- Terms of service -->
                  <p>By clicking
                    <em>Sign up</em> you agree to our
                    <a href="" target="_blank">terms of service</a>
                </form>
                <!-- Form -->
              </div>
            </div>
            <!-- Material form register -->
          </div>
          <!--Grid column-->
        </div>
        <!--Grid row-->
      </div>
    </div>
  </section>
  <!--Intro Section-->
</header>
