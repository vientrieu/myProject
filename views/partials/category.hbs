<style>
    #products img {
        width: 100%;
    }

    @media(min-width: 960px) {
        #products .col-md-3 .product-card {
            width: auto;
            height: 150px;
        }

        #products .col-md-3 .well {
            height: 350px;
        }

        #products .col-md-3 .view {
            width: 100%;
            height: 150px;
        }

        #products .col-md-3 img {
            width: auto;
            height: 100%;

        }
    }


    /*------/ Property Single /-----*/

    .gallery-property {
        margin-bottom: 3rem;
    }

    .property-price {
        margin: 0 auto;
    }

    .property-price .card-header-c {
        padding: 0;
    }

    .summary-list {
        padding-right: 1rem;
        color: #000000;
    }

    .summary-list .list {
        padding: 0;
        line-height: 2;
    }

    .summary-list .list span {
        color: #555555;
    }

    .property-agent .title-agent {
        font-weight: 600;
    }

    .property-agent ul {
        line-height: 2;
        color: #000000;
    }

    .property-agent .socials-a {
        text-align: center;
    }
</style>
<div class="col-md-9">
