<style>
    #products img {
        width: 100%;
    }

    @media(min-width: 960px) {
        #products .col-md .product-card {
            width: auto;
            height: 150px;
        }

        #products .col-md .well {
            height: 350px;
        }

        #products .col-md .view {
            width: 100%;

        }

        #products .col-md img {
            width: auto;
            height: 100%;

        }
    }
</style>
<div class="index-module productv2" style="background:#f9f9f9">
    <!-- 作品、案例、产品 -->
    <div class="backcolor workv4a ">
        <div class="workv4-h">
            <div class="clear" style="height:60px;"></div>
            {{!-- Top 5 sản phẩm sắp kết thúc --}}
            <div class="index_title text-center pb30 wow slideInUp">
                <h3 class="f30">Top 5 sản phẩm gần kết thúc</h3> <span class="i-back"></span>
            </div>
            <div class="container  text-black-50  ">
                <div id="products" class="row text-dark">
                    {{#each psByTimeout}}
                        <div class="product-card text-white col-md mx-2 pt-3 px-1 d-flex justify-content-center wow slideInUp card card-ecommerce"
                            data-wow-delay=".4s">
                            <div id={{ProID}} class="well text-center d-flex flex-column justify-content-center">
                                <!--Card view-->
                                <div onclick="getDetailProduct('{{ProID}}')"
                                    class="view overlay zoom d-flex justify-content-center">
                                    <img src="../../../sp/{{ProID}}/1.jpg" class="img-fluid" alt="zoom">
                                    <div class="mask flex-center waves-effect waves-light ">
                                        <p class="card-body text-white  rgba-black-strong p-2 align-self-end">
                                            {{ProName}}</p>
                                    </div>
                                </div>
                                <span class="badge badge-danger product ml-xl-0">Sắp hết thời gian</span>
                                <!-- Card time -->
                                <div class="rounded-top mdb-color lighten-3 text-center mt-3">
                                    <ul class="list-unstyled list-inline font-small p-1 m-0">
                                        <li class="list-inline-item pr-2 white-text"><i
                                                class="far fa-clock pr-1"></i>{{TimeFinish}}<i
                                                class="fas fa-hourglass-half pr-1"></i></li>
                                    </ul>
                                </div>
                                <!--Card content-->
                                <div class="orange text-white rounded-bottom m-0">
                                    <ul class="list-unstyled list-inline font-small p-1 m-0">
                                        <li>
                                            <ul
                                                class="list-unstyled list-inline font-small p-1 m-0 d-flex justify-content-between">
                                                <li class="list-inline-item pr-2 red-text font-weight-bold"><i
                                                        class="fas fa-hand-holding-usd pr-1"></i><strong>{{Price}}</strong>
                                                </li>
                                                <li class="list-inline-item pr-2 white-text"><i
                                                        class="fas fa-user-clock pr-1"></i>{{UserName}}
                                                </li>
                                            </ul>
                                        </li>
                                        <li>
                                            <ul
                                                class="list-unstyled list-inline font-small p-1 m-0 d-flex justify-content-between">
                                                <li class="list-inline-item pr-2 white-text"><i
                                                        class="fas fa-dollar-sign pr-1"></i>{{PriceToBuy}}
                                                </li>
                                                <li class="list-inline-item pr-2 white-text"><i
                                                        class="fas fa-hammer pr-2"></i>{{Voted}}
                                                </li>
                                                <li>
                                                    <span class="card-footer float-right text-white p-0 mr-2">
                                                        <a class="heart-icon" data-toggle="tooltip" data-placement="top"
                                                            title="Added to Wishlist" data-id={{ProID}}
                                                            data-name={{ProName}} data-price={{Price}}><i
                                                                class="fas fa-heart"></i></a>
                                                    </span>
                                                </li>
                                            </ul>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    {{/each}}
                </div>
            </div>
            {{!-- Top 5 sản phẩm có nhiều lượt ra giá nhất --}}
